  "components": {
    "schemas": {
      "CarReqBody": {
        "required": ["name", "price", "type", "image"],
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "example": "avanza"
          },
          "price": {
            "type": "integer",
            "example": 200000
          },
          "type": {
            "type": "string",
            "example": "small"
          },
          "image": {
            "type": "string",
            "example": "cars.jpg"
          }
        }
      },
      "CarPostResponse": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int64",
            "example": 1
          },
          "name": {
            "type": "string",
            "example": "avanza"
          },
          "price": {
            "type": "integer",
            "format": "int64",
            "example": 10000
          },
          "type": {
            "type": "string",
            "example": "medium"
          },
          "image": {
            "type": "string",
            "example": "cars.jpg"
          },
          "isDeleted": {
            "type": "boolean",
            "example": false
          },
          "createdBy": {
            "type": "string",
            "example": "superadmin@mail.com"
          },
          "updatedBy": {
            "type": "string",
            "example": "superadmin@mail.com"
          },
          "createdAt": {
            "type": "string",
            "example": "2022-10-22 14:02:14.678+07"
          },
          "updatedAt": {
            "type": "string",
            "example": "2022-10-22 14:02:14.678+07"
          },
          "deletedBy": {
            "type": "string",
            "example": null
          }
        }
      },
      "CurrentUser": {
        "type": "object",
        "properties": {
