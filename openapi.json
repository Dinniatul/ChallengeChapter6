          },
          "password": {
            "type": "string",
            "example": "$2a$10$pLMVso373KqFVztb/H5/2eJaSDn1VR37E2N3uIwIURq0QxDEoQWaW"
          },
          "role": {
            "type": "string",
            "example": "admin"
